      "supportsTablet": false,
      "bundleIdentifier": "com.fruchtiapps.breakup",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "package": "com.fruchtiapps.breakup",
      "versionCode": 1,
      "permissions": [],
      "icon": "./assets/icon.png",
      "splash": {
        "image": "./assets/logo2.png",
        "resizeMode": "contain",
        "backgroundColor": "#faf7f2"
      }
    }
  }
}
